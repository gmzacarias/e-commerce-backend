{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "baseUrl": ".",
    "paths": {
      "components/*": [
        "components/*"
      ],
      "controllers/*": [
        "controllers/*"
      ],
      "lib/*": [
        "lib/*"
      ],
      "models/*": [
        "models/*"
      ],
      "pages/*": [
        "pages/*"
      ],
      "repositories/*": [
        "repositories/*"
      ],
      "services/*": [
        "services/*"
      ],
      "utils/*": [
        "utils/*"
      ]
    },
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strict": false
  }
}